<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "sql-map-2.dtd">

<sqlMap namespace="ClientFeature">

    <select id="getClientFeatureById" parameterClass="java.lang.String" resultClass="com.archermind.txtbl.domain.ClientFeature">
        select id, name, descriptions from feature where id=#id#
    </select>

</sqlMap>